<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuizDickTionary.Application.Views.StartQuizWord"
             xmlns:dt="clr-namespace:QuizDickTionary.Application.ViewModels.Components"
             x:DataType="dt:WordViewModel">
    <VerticalStackLayout Background="Transparent">
        <Label Text="{Binding Hungarian}"></Label>
        <Label Text="{Binding Slovak}"></Label>
        <Label Text="{Binding English}"></Label>
        <Label Text="{Binding Difficulty}"></Label>
    </VerticalStackLayout>
</ContentView>
