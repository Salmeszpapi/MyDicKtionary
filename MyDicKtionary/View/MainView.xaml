             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyDicKtionary.View.MainView"
             Title="MainView">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <ContentView Grid.Row="0" Content="{Binding CurrentView}" />
    </Grid>
</ContentPage>